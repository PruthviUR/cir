public class Main {
    public static void main(String[] args) {
        CircularLInkedList cll = new CircularLInkedList();

        cll.display();
        cll.insert(10);
        cll.display();
        cll.insert(20);
        cll.display();
        cll.insert(30);
        cll.display();
        cll.insert(40);
        cll.display();
cll.deleteByPosition(1);
        cll.display();

    }
